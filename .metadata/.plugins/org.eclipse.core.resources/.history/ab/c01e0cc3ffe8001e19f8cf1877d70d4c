package Thuchanhbuoi4;

public class SDConVat {

	public static void main(String[] args) {
		Bo bo = new Bo();
		Ga ga = new Ga();
		De de = new De();
		Heo heo = new Heo();
		
		bo.NhapThongTin();
		heo.NhapThongTin();
		de.NhapThongTin();
		ga.NhapThongTin();
		
		bo.Hienthi();
		bo.Keu();

		de.Hienthi();
		de.Keu();

		heo.Hienthi();
		heo.Keu();

		ga.Hienthi();
		ga.Keu();
	}

}
